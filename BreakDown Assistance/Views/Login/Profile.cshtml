@model IEnumerable<BreakDown_Assistance.Models.Admin>
@{

    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}
<!DOCTYPE html>

<head>

    <style>
        .header2 {
            overflow: hidden;
            background-color: whitesmoke;
            padding-top: 21px;
            padding: 5px 0px;
            padding-right: 16px;
            width: 1600px;
            border-bottom: groove;
            border-bottom-color: beige;
            margin-left: 300px;
        }

            .header2 a {
                float: left;
                color: black;
                text-align: center;
                padding: 12px 7px;
                text-decoration: none;
                font-size: 20px;
                line-height: 25px;
                border-radius: 1px;
            }

                .header2 a.logo {
                    font-size: 20px;
                    font-weight: normal;
                }

                .header2 a:hover {
                    background-color: dodgerblue;
                    color: white;
                }

                .header2 a.active {
                    background-color: dodgerblue;
                    color: white;
                }

        .header2-right {
            margin-left: 0px;
        }

        .main {
            /* Same as the width of the sidenav */
            background-color: whitesmoke;
        }
    </style>
</head>

<div class="header2">
    <a href="/Login/Profile" class="logo">
        <i class="fa fa-home">
            Home
        </i>
    </a>
</div>
<center><h1>Profile Info</h1></center>
<hr />
<div class="header2">

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Password)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Password)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>



